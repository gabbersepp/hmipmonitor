@model long
@{
    ViewData["Title"] = "Details";
}



<h1>@ViewData["Title"]</h1>

<div class="parameter-main">

</div>

@section scripts{
    <script>
        async function load() {
            while (true) {
                try {
                    await $(".parameter-main").load("/Dashboard/CarPartial/@Model");
                    await new Promise(resolve => {
                        setTimeout(resolve, 5000)
                    });
                } catch (e) {
                    console.error(e);
                }
            }
        }

        load();
    </script>
}